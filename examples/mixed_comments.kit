include "stdio.h";

// Mix of line and block comments
function main() {
    // Start with line comment
    var x: Int = 1;

    /* Block comment in middle */
    var y: Int = 2; // End of line comment

    /*
     * Multi-line block comment
     * followed by line comment
     */
    // After multi-line
    var z: Int = x + y;
    printf("%d", z); /* Block comment at end */
}
